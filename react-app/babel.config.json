{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["transform-inline-environment-variables"],
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".android.js",
          ".ios.js"
        ],
        "alias": {
          "root": "./",
          "config": "./src/config",
          "library": "./src/library",
          "rdx": "./src/redux",
          "res": "./src/res",
          "screens": "./src/screens"
        }
      }
    ]
  ]
}